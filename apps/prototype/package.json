{
    "name": "fastify-autoload-app",
    "version": "1.0.0",
    "description": "Fastify app with autoload plugin",
    "main": "dist/app.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/src/app.js",
        "dev:appa": "APP=appa tsc && bun --watch --env-file=.env dist/src/app.js",
        "dev:appb": "APP=appb sh -c 'tsc && bun --watch --env-file=.env dist/src/app.js'",
        "dev:appc": "APP=appc sh -c 'tsc && bun --watch --env-file=.env dist/src/app.js'",
        "seed": "tsx src/seed.ts",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate",
        "db:push": "drizzle-kit push",
        "db:studio": "drizzle-kit studio"
    },
    "dependencies": {
        "@fastify/autoload": "^5.8.0",
        "dotenv": "^17.2.3",
        "drizzle-orm": "^0.44.7",
        "fastify": "^4.24.3",
        "ioredis": "^5.8.2",
        "pg": "^8.16.3",
        "stream-json": "^1.9.1",
        "zod": "^4.1.11"
    },
    "devDependencies": {
        "@types/ioredis": "^4.28.10",
        "@types/node": "^20.0.0",
        "@types/pg": "^8.15.5",
        "@types/stream-json": "^1.7.8",
        "drizzle-kit": "^0.31.5",
        "ts-node": "^10.9.0",
        "tsx": "^4.20.6",
        "typescript": "^5.0.0"
    }
}
